
    <table class="table">
      <thead>
          <tr>
            <th>Date</th>
            <th>Video (Source)</th>
            {#<th>Video (720p)</th>#}
            {#<th>Video (480p)</th>#}
            {#<th>Video (360p)</th>#}
            {#<th>Video (240p)</th>#}
            {#<th>Audio Only</th>#}
            <th>Thumbnail</th>
            <th>Title</th>
            <th>Annouce URL</th>
          </tr>
        </thead>
        <tbody>
          {% for vod in vodslist | reverse %}
          <tr>
            <td>{{ vod.data.date | htmlDateString }}</td>
            
            <td>
              {% if vod.data.videoSrcHash %}
                <a target="_blank" href="{% buildIpfsUrl vod.data.videoSrcHash %}">
                    <span>
                      <i class="fas fa-video"></i>
                    </span>
                    <span class="icon">
                      <i class="fas fa-external-link-alt"></i>
                    </span>
                </a>
              {% endif %}
            </td>

            {#<td>
