---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - vod
    - vods
    - tagList
  addAllPagesToCollections: true
layout: layouts/base.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /tags/{{ tag }}/
---


<div class="content box">
  <h1>Tagged “{{ tag }}”</h1>

  {% set vodslist = collections[ tag ] %}
  {% include "vodslist.njk" %}

  <p>See <a href="{{ '/tags/' | url }}">all tags</a>.</p>

</div>