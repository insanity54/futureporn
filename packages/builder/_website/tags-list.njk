---
permalink: /tags/
layout: layouts/base.njk
---

<div class="content">
  <div class="box">
    <h2>Tags</h2>
    <div class="tags">
      {% for tag in db.tags %}
        {% set tagUrl %}/tags/{{ tag }}/{% endset %}
        <span class="tag">
          <a href="{{ tagUrl | url }}" class="vod-tag">{{ tag }}</a>
        </span>
      {% endfor %}
    </div>
  </div>
</div>