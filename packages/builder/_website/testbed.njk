<h2>video.js</h2>
<video
    id="my-video"
    class="video-js"
    controls
    preload="auto"
    width="640"
    height="264"
    data-setup="{}"
  >
    <source src="{{playbackId}}.m3u8?token={{token}}" type="video/mux" />
    <p class="vjs-no-js">
      To view this video please enable JavaScript, and consider upgrading to a
      web browser that
      <a href="https://videojs.com/html5-video-support/" target="_blank"
        >supports HTML5 video</a
      >
    </p>
  </video>


<h2>vidstack</h2>
  <media-player
    load="eager"
    title="vidstack test"
    preload="metadata"
    aspect-ratio="16/9"
    style="--media-aspect-ratio: 1.7778;"
    src="https://stream.mux.com/{{playbackId}}.m3u8?token={{token}}"
    controls
  >
    <media-outlet>
    </media-outlet>
    <media-poster></media-poster>
    <media-community-skin></media-community-skin>
  </media-player>

<h2>mux player</h2>
<mux-player
  stream-type="on-demand"
  playback-id="{{ playbackId }}"
  playback-token="{{ token }}"
  metadata-video-title="Test VOD"
