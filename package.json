{
  "name": "futureporn",
  "version": "1.0.0",
  "description": "https://futureporn.neocities.org",
  "engineStrict": "true",
  "engines": {
    "node": "^12.20.0 || >=14.13.1"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:assets && yarn run build:site",
    "build:assets": "NODE_ENV=production webpack --mode=production",
    "build:site": "NODE_ENV=production eleventy --config=./.eleventy.cjs",
    "clean": "rm -rf ./_site",
    "deploy": "npm run build:assets && npm run build:site && node publish.cjs",
    "publish": "npm run deploy",
    "dev": "npm-run-all clean webpack:assets --parallel dev:*",
    "dev:assets": "yarn run webpack:assets --watch",
    "dev:site": "NODE_ENV=development eleventy --serve --config=./.eleventy.cjs",
    "webpack:assets": "NODE_ENV=development webpack --mode=development",
    "test": "mocha",
    "down": "ansible-playbook -i ./vultr.yml destroy.yml",
    "up": "ansible-playbook -i vultr.yml -e \"instances=1\" site.yml",
    "bigup": "ansible-playbook -i vultr.yml -e \"instances=3\" site.yml",
    "gateway": "./ansible-playbook.sh -i vultr.yml gateway.yml",
    "start": "node futureporn.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insanity54/futureporn.git"
  },
  "author": "Chris Grimmett <chris@grimtech.net>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/insanity54/futureporn/issues"
  },
  "homepage": "https://github.com/insanity54/futureporn#readme",
  "dependencies": {
    "@grimtech/envimport": "^0.0.1",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.1.6",
    "debug": "^4.3.1",
    "decode-uri-component": "^0.2.0",
    "dotenv": "^10.0.0",
    "execa": "^5.1.1",
    "fdir": "^5.1.0",
    "globby": "^11.0.4",
    "gray-matter": "^4.0.3",
    "markdown-it-anchor": "^8.1.0",
    "mini-css-extract-plugin": "^2.1.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.6",
    "prevvy": "^4.0.0",
    "process": "^0.11.10",
    "ramda": "^0.27.1",
    "twitter-v2": "^1.1.0"
  },
