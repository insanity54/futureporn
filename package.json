{
  "name": "futureporn-cb-chat",
  "version": "0.0.1",
  "description": "https://futureporn.net",
  "engineStrict": "true",
  "engines": {
    "node": "^12.20.0 || >=14.13.1"
  },
  "config": {
    "libvips": "8.13.3"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:assets && yarn run build:site",
    "build:patrons": "node ./patreon.js",
    "build:assets": "NODE_ENV=production webpack --mode=production",
    "build:site": "NODE_ENV=production eleventy --config=./.eleventy.cjs",
    "patreon": "npm run build:patrons",
    "clean": "rm -rf ./_site",
    "deploy": "npm run build:assets && npm run build:site && node publish.cjs",
    "publish": "npm run deploy",
    "dev": "npm-run-all clean webpack:assets --parallel dev:*",
    "ngrok": "ngrok http --region=us --hostname=futureporn.ngrok.io 8000",
    "dev:assets": "yarn run webpack:assets --watch",
    "dev:site": "NODE_ENV=development eleventy --serve --port 9898 --config=./.eleventy.cjs",
    "webpack:assets": "NODE_ENV=development webpack --mode=development",
    "test": "mocha",
    "down": "ansible-playbook -i ./vultr.yml destroy.yml",
    "capture": "./ansible-playbook.sh -i vultr.yml capture.yml",
    "manager": "./ansible-playbook.sh -i vultr.yml manager.yml",
    "cluster": "./ansible-playbook.sh -i vultr.yml cluster.yml",
    "gateway": "./ansible-playbook.sh -i vultr.yml gateway.yml",
    "uppy": "./ansible-playbook.sh -i vultr.yml uppy.yml",
    "start": "node futureporn.mjs",
    "light-server": "light-server -s ./_site/ -p 8888",
    "lighthouse": "lighthouse localhost:8888"
  },
